romimage: file=/usr/share/bochs/BIOS-bochs-latest
megs: 1024
ata1-slave: type=cdrom, path="build/exocore.iso", status=inserted
boot: cdrom
cpu: count=2:4, reset_on_triple_fault=0
magic_break: enabled=1
debug_symbols: file=build/exocore.sym
panic: action=report
error: action=report
info: action=report
